<wizard category="B.FMFClasses" id="A.ContextualWidgetManager" kind="class" version="1">
    <displayname>Contextual widget manager</displayname>
    <displayname xml:lang="de">Manager von Kontextualisierten Widgets</displayname>
    <displayname xml:lang="fr">Gestionnaire de widgets contextuels</displayname>
    <icon>../shared/preferences-system.png</icon>
    <description>Manager of contextual widgets, and action handler.</description>
    <description xml:lang="fr">Gestionnaire de widgets contextuels et d'actions.</description>
    <description xml:lang="de">Manager von Kontextualisierten Widgets, und dazugehöriger ActionHandler</description>
    <displaycategory>FreeMedForms</displaycategory>
    <files>
        <file openeditor="true" source="contextualwidgetmanager.cpp.tpl" target="%PluginName:l%contextualwidgetmanager.%CppSourceSuffix%" />
        <file openeditor="true" source="contextualwidgetmanager.h.tpl" target="%PluginName:l%contextualwidgetmanager.%CppHeaderSuffix%" />
    </files>

    <fieldpagetitle>Manager parameters</fieldpagetitle>
    <fieldpagetitle xml:lang="fr">Paramètres du gestionnaire</fieldpagetitle>
    <fieldpagetitle xml:lang="de">Manager-Parameter</fieldpagetitle>
    <fields>
        <field name="PluginName">
            <fieldcontrol class="QComboBox" combochoices="Account,AccountDB,Agenda,Alert,BaseWidgets,Category,Core,DataPackPlugin,DrugInteractions,DrugsDB,DrugsWidget,Editor,Empty,Form,Gir,ICD,MainWin,PMH,PadTools,Patients,Print,SaveRestore,Script,Templates,UserPlugin,Views,Webcam,XmlForms,ZipCodes" defaultindex="-1" />
            <fielddescription>Plugin:</fielddescription>
            <fielddescription xml:lang="de">Plugin:</fielddescription>
            <fielddescription xml:lang="fr">Greffon:</fielddescription>
        </field>
        <field name="Author">
            <fieldcontrol class="QLineEdit" defaulttext="" />
            <fielddescription>Author:</fielddescription>
            <fielddescription xml:lang="de">Autor:</fielddescription>
            <fielddescription xml:lang="fr">Auteur:</fielddescription>
        </field>
        <field name="AuthorEmail">
            <fieldcontrol class="QLineEdit" defaulttext=""/>
            <fielddescription>Author email:</fielddescription>
            <fielddescription xml:lang="de">Emailadresse des Autors:</fielddescription>
            <fielddescription xml:lang="fr">Adresse émail de l'auteur:</fielddescription>
        </field>
    </fields>
</wizard>
