<MedForm name="Test::DropList" uifilename="./ui/xx/listdrop.ui">
  <label lang="xx">DropList</label>
  <label lang="fr">DropList</label>
  <options>UseFormNameAsNameSpace</options>
  <icon>__theme__/stethoscope.png</icon>
  <!--file type="htmlprintmask">./html/xx/.html</file-->
